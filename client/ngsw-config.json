{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Roast.li",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data",
      "urls": ["https://api.roast.li/**"],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    }
  ]
}
